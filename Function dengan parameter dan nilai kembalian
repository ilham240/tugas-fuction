#include <iostream>
using namespace std;

void cetakinfo ()
{
	cout << "Nama : Muhammad ilham" <<endl;
	cout << "nim : ETE10190105" <<endl;
	cout << "Prodi : Teknik Elektromedis" <<endl;
	cout << "Mata Kuliah : Bahasa Pemograman" <<endl;
	cout << endl;
	cout << "Tugas tentang Function dengan parameter dan nilai kembalian" <<endl;
}

int pengurangan (int a, int b) {
				return a - b;
}

int main ()
{
	cetakinfo ();
	cout << "Berikut adalah hasil pengurangan dari (176,20):" << pengurangan (176,20);
	return 0;
}
