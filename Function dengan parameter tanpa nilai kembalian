#include <iostream>
using namespace std;

//Function dengan parameter tanpa nilai kembalian
void hasilPerkalian (int nilai1, int nilai2) 
{
		cout << "Nama: Muhammad Ilham" <<endl;
		cout << "NIM: ETE10190105" <<endl;
		cout << "Prodi: Teknik Elektromedis" <<endl;
		cout << "Mata Kuliah: Bahasa Pemrograman" <<endl;
		cout << endl;
		cout << "Tugas tentang Function dengan parameter tanpa nilai kembalian" <<endl;
		cout << "Berikut adalah hasil perkalian dari (15,10):" <<endl << (nilai1 * nilai2);
}
//ini adalah function utama
int main () 
{
	hasilPerkalian (15,10);
		return 0;
}
